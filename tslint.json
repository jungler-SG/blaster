{
  "extends": "tslint:recommended",
  "rules": {
    "no-console": false,
    "no-var-requires": false,
    "max-line-length": [
      150
    ],
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_"
      }
    ],
    "ordered-imports": false,
    "no-trailing-whitespace": [
      true,
      "ignore-comments"
    ]
  }
}
